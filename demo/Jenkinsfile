pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'   
                 bat 'set'
            }
        }
       stage('build') {
          steps {
             bat 'echo step1'
             bat './gradlew.bat check'
               bat 'echo step2'
             bat '''
                echo 'Multiline'
                echo 'Example'
             '''
             echo 'not using shell'
          }
       }
    }
    post { 
        always { 
             echo 'I will always say Hello again!'
        }
    }
}
