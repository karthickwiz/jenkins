pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'   
                 bat 'set'
            }
        }
       stage('build') {
          steps {
             bat 'echo building....'
             bat 'gradle build'
               bat 'echo builded....'    
            
          }
       }
    }
    post { 
        always { 
             echo 'I will always say Hello again!'
        }
    }
}
