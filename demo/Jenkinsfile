pipeline {
    agent any
    stages {
        stage('Initialize') {
            steps {
                echo 'setting...'   
                 bat 'set'
            }
        }
       stage('build') {
          steps {
             bat 'echo building....'
             bat 'gradle build'
               bat 'echo builded....'    
            
          }
       }
        stage('try') {
          steps {
             bat 'echo changing path....'
           //  bat 'cd E:\\git\\repo\\demo'
          //   bat 'gradle build'
               bat 'echo builded....'    
            
          }
       }
   stage('SonarQube analysis') {
        steps {
        withSonarQubeEnv('clsd') {
      // requires SonarQube Scanner for Gradle 2.1+
      // It's important to add --info because of SONARJNKNS-281
        bat 'gradlew --info sonarqube'
        
        echo 'sonar report'
       //   bat 'gradle build --info sonarqube'
    }
  }
   }
    }
    post { 
        always { 
             echo 'DONE!'
        }
    }
}
