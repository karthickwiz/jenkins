pipeline {
    agent any
    stages {
        stage('Initialize') {
            steps {
                echo 'setting...'   
                 bat 'set'
            }
        }
       stage('build') {
          steps {
             bat 'echo building....'
             bat 'gradle build'
               bat 'echo builded....'    
            
          }
       }
        stage('try') {
          steps {
             bat 'echo changing path....'
           //  bat 'cd E:\\git\\repo\\demo'
          //   bat 'gradle build'
               bat 'echo builded....'    
            
          }
       }
 stage('sonarqube') {
                    steps {
                             script {
                                    def scannerHome = tool 'SonarQube_Scanner';
                                    withSonarQubeEnv('SonarQube') {
                                    sh "${scannerHome}/bin/sonar-scanner\"
                                    
                                     }
                               }
                           }              
                        }
    }
    post { 
        always { 
             echo 'DONE!'
        }
    }
}
